# AI Software Engineer Docker Image Environment Variables
# Copy this file to .env and modify as needed

# Docker image configuration
IMAGE_NAME=ai-software-engineer
IMAGE_TAG=latest
CONTAINER_NAME=ai-dev-env

# Registry configuration (for pushing images)
REGISTRY_USERNAME=your-username
REGISTRY_URL=docker.io

# GitHub configuration
GITHUB_TOKEN=your-github-token
GITHUB_USERNAME=your-github-username

# Development configuration
NODE_ENV=development
PYTHON_ENV=development

# Network configuration
HTTP_PORT=8000
DEV_PORT=3000
ALT_PORT=8080

# Volume configuration
WORKSPACE_PATH=./
CACHE_PATH=~/.cache/ai-software-engineer

# AI Tools configuration
# GitHub Copilot - requires GitHub authentication
GITHUB_COPILOT_ENABLED=true

# Alibaba Qoder - configure if available
QODER_ENABLED=true
QODER_API_KEY=your-qoder-api-key

# Cursor - configure if available
CURSOR_ENABLED=true
CURSOR_API_KEY=your-cursor-api-key

# ByteDance Trae - configure if available
TRAE_ENABLED=true
TRAE_API_KEY=your-trae-api-key

# Optional: Database configuration (if using database services)
POSTGRES_DB=aidev
POSTGRES_USER=aidev
POSTGRES_PASSWORD=aidev123

# Optional: Redis configuration (if using Redis)
REDIS_URL=redis://localhost:6379